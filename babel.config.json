{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "env": {
    "commonjs": {
      "presets": [["@babel/preset-env", { "modules": "commonjs" }]]
    },
    "modules": {
      "presets": [["@babel/preset-env", { "modules": false }]]
    },
    "default": {
      "presets": [["@babel/preset-env", { "modules": false }]]
    }
  }
}
